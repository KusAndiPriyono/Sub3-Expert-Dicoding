{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "es6": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },

  "parserOptions": {
    "ecmaVersion": 14,
    "sourceType": "module",
    "class-methods-use-this": 0,
    "no-new": 0
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-console": 0,
    "no-underscore-dangle": "off"
  },
  "plugins": ["codeceptjs", "jasmine"]
}
